# socratic_ai_tutor/.env.example
# Backend Production Settings

# Required: Change this to a long random secret string
JWT_SECRET_KEY=yoursecretkeyhere

# Optional: List of allowed origins for CORS (comma separated)
# CORS_ORIGINS=https://yourdomain.com,app://socratic-tutor

# LLM Model Settings
MODEL_PATH=/app/models/socratic-q4_k_m.gguf
N_THREADS=4
N_GPU_LAYERS=0
N_CTX=4096

# Admin Credentials (Set these to override defaults in config.json)
# ADMIN_EMAIL=admin@example.com
# ADMIN_PASSWORD=securepassword
